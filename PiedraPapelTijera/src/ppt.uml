      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_zZrZ4qFhEeyrT6E3m1PbaA" name="getMove" specification="_zZCgsaFhEeyrT6E3m1PbaA">
        <ownedParameter xmi:id="_zZrZ46FhEeyrT6E3m1PbaA" type="_zY0eQKFhEeyrT6E3m1PbaA" direction="return"/>
        <language>java</language>
        <body>String command = null;
	    System.out.print(&quot;Tu elecciÃ³n: &quot;);
	    
	    String input = reader.nextLine();

	    Scanner tokenizer = new Scanner(input);
     
	    if(tokenizer.hasNext()) {
	        // get first word
             command = tokenizer.next().toUpperCase(); 
             if(tokenizer.hasNext()) {
                 // note: if the command has two or more words, it is not
		            // valid
		            return new Move();
             }
	    }
	    
	    if ( (command != null) &amp;&amp; (moves.isValidMoveCommand(command)) )
	        return new Move(command);
	    
	    // we return a move with no associated command, as it is unknown.
	    return new Move();
 </body>
