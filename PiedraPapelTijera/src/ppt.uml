        </ownedParameter>
        <language>java</language>
        <body>Move pcMove;

		if (move.isUnknown()) {
			System.out.println(&quot;OpciÃ³n No VÃ¡lida&quot;);
			printHelp();
			return true;
		}

		if (move.getMove().equals(&quot;SALIR&quot;)) {
			return false;
		}

		if (move.getMove().equals(&quot;AYUDA&quot;)) {
			printHelp();
			return true;
		}
		pcMove = computerMove();
		printMoves(move, pcMove);
		printWinner(move.beats(pcMove));
		return true;
	</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_zZo9pKFhEeyrT6E3m1PbaA" name="playGame" specification="_zYwz4KFhEeyrT6E3m1PbaA">
        <language>java</language>
        <body>boolean continuing = true;
		Move move;
		int round = 1;
		while (continuing) {
			System.out.println(&quot;****** &quot; + round + &quot; turno ******&quot;);
			move = parser.getMove();
			continuing = processMove(move);
			round++;
		}
		System.out.println(&quot;Adios!!&quot;);
	</body>
      </ownedBehavior>
